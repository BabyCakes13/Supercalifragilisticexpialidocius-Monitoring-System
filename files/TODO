    def test_get_metrics_dictionary(self):
        """Tests if get_metrics_dictionary() calls the correct functions for a specific input."""

        mocker = MagicMock()

        with patch('metrics.collect_metrics.Metrics.get_disk_usage', mocker.function1), \
                patch('metrics.collect_metrics.Metrics.get_cpu_percent', mocker.function2), \
                patch('metrics.collect_metrics.Metrics.get_memory_info', mocker.function3), \
                patch('metrics.collect_metrics.Metrics.get_cpu_stats', mocker.function4):

            expected = [call.function1(),
                        call.function2(),
                        call.function3(),
                        call.function4()]

        collector = Metrics()
        collector.get_metrics_dictionary()

        self.assertEqual(mocker.mock_calls, expected)
